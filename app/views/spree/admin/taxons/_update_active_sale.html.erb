<h1>Active Sale Events</h1>
<table>
<thead>
<th>Name</th>
<th>Start Date</th>
<th>End Date</th>
<th>PO Sale ID</th>
<th>Ship Config</th>
<th>Vendor Code</th>

</thead>
<tbody>

<% if @active_sale_events %>
	<% @active_sale_events.each do |active_sale| %>
	<tr>
	<td><%= active_sale.name %></td>
	<td><%= active_sale.start_date %></td>
	<td><%= active_sale.end_date %></td>
	<td><%= active_sale.po_sale_id %></td>
	<td><%= active_sale.ship_configuration_id %></td>
	<td><%= active_sale.vendor_code %></td>
	<td><%= link_to "Edit", "/admin/active_sales/#{active_sale.active_sale_id}/active_sale_events/#{active_sale.id}/edit" ,:id=>"active_sale_#{active_sale.id}", :class => " active_sale_class" %> </td>
	</tr>
<% end %>
<% end %>

</tbody>
</table>
